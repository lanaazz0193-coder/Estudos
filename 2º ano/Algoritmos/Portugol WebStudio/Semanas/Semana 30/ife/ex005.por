//Ana Lívia
programa {

  inteiro pg, v, tl

  funcao inicio() {

    escreva("Digite o preço do litro da gasolina: ")
    leia(pg)
    escreva("Digite o valor que você pretende gastar para abastecer: ")
    leia(v)

    tl = v / pg

    escreva("Com esse valor você pode comprar ", tl, " de gasolina.")

    
  }
}
