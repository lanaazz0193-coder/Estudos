programa {

  real n1, n2, n3, m

  funcao inicio() {

    escreva("Digite a primera nota: ")
    leia(n1)
    escreva("Digite a segunda nota: ")
    leia(n2)
    escreva("Digite a terceira nota: ")
    leia(n3)

    m = (n1 + n2 + n3) / 3

    escreva("A média dessas notas é ", m)
    
  }
}
