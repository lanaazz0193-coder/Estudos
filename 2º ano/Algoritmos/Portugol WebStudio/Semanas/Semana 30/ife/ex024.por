programa {

  real l, a, s

  funcao inicio() {

    escreva("Digite a quantidade em litros de refresco você deseja fazer: ")
    leia(l)

    a = (l / 10) * 8
    s = (l / 10) * 2

    escreva("Para essa quantidade de refresco serão necessários ", a, " litros de água e ", s, " de suco de maracujá.")
    
  }
}
