programa {

  cadeia filme, lancamento, diretor

  funcao inicio() {

      escreva("Título do Filme: ", filme) //Espere o usuário digitar
      leia(filme)
      escreva("Ano de Lançamento: ", lancamento) //Espere o usuário digitar
      leia(lancamento)
      escreva("Diretr(a): ", diretor) //Espere o usuário digitar
      leia(diretor)
  }
}
